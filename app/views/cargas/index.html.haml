.page-header
  .row
    .col-md-7
      %h1 Rutas
    .col-md-5
      =link_to "Nueva Carga", new_ruta_path, remote: true, class: "btn btn-primary btn-lg pull-right"

%table.table
  %thead
    %tr
      %th Fecha 
      %th Nombre de archivo
  %tbody
    -@cargas.each do |proceso|
      %tr
        %td=proceso.created_at
        %td=proceso.name

=render 'layouts/modal'